{
    "name": "pantz/orm",
    "description": "Pantz ORM",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan George",
            "email": "ryangeorge.9@gmail.com"
        }
    ],
    "require": {
        "php": "7.1.1",
        "php-console/php-console": "3.1.6",
        "doctrine/orm": "2.5.9",
        "symfony/yaml": "3.3.0",
        "symfony/serializer": "3.3.9",
        "symfony/property-access": "3.3.9"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}